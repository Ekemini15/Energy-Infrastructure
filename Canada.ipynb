    "pp.create_line_from_parameters(net, from_bus =EVs_station_transmission, to_bus =  EVs_station_distribution, length_km = 0.265, r_ohm_per_km = 0.916, x_ohm_per_km = 1.574, c_nf_per_km = 1e-9, max_i_ka = 0.242, name = \"central transmission to  EVs_station distribution\" )\n",
    "pp.create_line_from_parameters(net, from_bus =EVs_station_distribution, to_bus =  EVs_station_load, length_km = 0.265, r_ohm_per_km = 0.979, x_ohm_per_km = 0.872, c_nf_per_km = 1e-9, max_i_ka = 0.184, name = \"central transmission to  EVs_station distribution\" )\n",
